import mysql.connector as mc
con = mc.connect(host = "localhost",user = "root",password= "root")
cur = con.cursor()
cur.execute("Use School;")
st = "Select name,class from students;"
cur.execute(st)
r = cur.fetchall()
print("Records data:")
print("name\t\t","class")
for i in r:
  print(i[0],"\t",i[1])
con.close()
